{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*.js"
      ]
    },
    "build-types": {
      "dependsOn": [
        "^build-types"
      ],
      "outputs": [
        "dist/**/d.ts",
        "dist/**/d.ts.map"
      ]
    },
    "release-build": {
      "dependsOn": [
        "^build",
        "^build-types"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "fmt": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}